<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platfos.pongift.data.gim.dao.GIMExhibitInfoDAO">
	<select id="selectGIMExhibitInfoListByGoodsCategoryAndChannelGb" resultType="com.platfos.pongift.data.gim.model.GIMExhibitInfo">
		SELECT *
		FROM gim_exhibit_info_tb
		WHERE goods_category = #{goodsCategory}
		AND channel_gb = #{channelGb}
	</select>
</mapper>